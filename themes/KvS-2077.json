{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "author": "notKvS",
  "name": "KvS Cyberpunk 2077",
  "themes": [
    {
      "name": "KvS Cyberpunk 2077",
      "appearance": "dark",
      "style": {
        "background": "#03102c",
        "status_bar.background": "#03102c",
        "elevated_surface.background": "#0e0952",
        "surface.background": "#0e0952",
        "search.match_background": "#003496",
        "border": "#181657",
        "border.variant": "#181657",
        "border.focused": "#0a2f66",
        "border.selected": "#181657",
        "border.transparent": "#181657",
        "border.disabled": "#181657",
        "element.background": "#087eb4",
        "element.selected": "#073170",
        "element.active": "#0a2f66",
        "element.hover": "#0a2f66",
        "element.disabled": "#181657",
        "error": "#ac077a",
        "error.background": "#03102c",
        "error.border": "#181657",
        "drop_target.background": "#063475",
        "ghost_element.background": "#00000000",
        "ghost_element.selected": "#073170",
        "ghost_element.active": "#0a2f66",
        "ghost_element.hover": "#0a2f66",
        "ghost_element.disabled": "#00000000",
        "text": "#ffffff",
        "text.muted": "#3e8efd",
        "text.placeholder": "#3e8efd",
        "text.disabled": "#0a2f66",
        "text.accent": "#0ef3ff",

        "icon": "#3e8efd",
        "icon.muted": "#3e8efd",
        "icon.disabled": "#181657",
        "icon.placeholder": "#0a2f66",
        "icon.accent": "#0ef3ff",

        "panel.background": "#03102c",
        "panel.focused_border": "#181657",
        "pane.focused_border": "#181657",
        "scrollbar.thumb.background": "#fc309654",
        "scrollbar.thumb.hover_background": "#ee0077",
        "scrollbar.thumb.border": "#fc309654",
        "scrollbar.track.background": "#03102c",
        "scrollbar.track.border": "#181657",
        "title_bar.background": "#03102c",
        "toolbar.background": "#03102c",
        "tab_bar.background": "#03102c",
        "tab.active_background": "#1d1641",
        "tab.inactive_background": "#141138",

        "editor.background": "#03102c",
        "editor.gutter.background": "#03102c",
        "editor.active_line.background": "#1c1347",
        "editor.foreground": "#ff2e97",
        "editor.subheader.background": "#03102c",
        "editor.highlighted_line.background": "#1c1347",
        "editor.line_number": "#ee0077",
        "editor.active_line_number": "#ff2e97",
        "editor.invisible": "#181657",
        "editor.wrap_guide": "#181657",
        "editor.active_wrap_guide": "#181657",
        "editor.document_highlight.read_background": "#0a2f66",
        "editor.document_highlight.write_background": "#073170",

        "terminal.background": "#03102c",
        "terminal.foreground": "#fdfeff",
        "terminal.bright_foreground": "#fdfeff",

        "terminal.ansi.black": "#2e1250",
        "terminal.ansi.bright_black": "#5a4a7a",
        "terminal.ansi.dim_black": "#2e1250",

        "terminal.ansi.red": "#ee1682",
        "terminal.ansi.bright_red": "#ff2e97",
        "terminal.ansi.dim_red": "#ac077a",

        "terminal.ansi.green": "#06ad00",
        "terminal.ansi.bright_green": "#3dd69c",
        "terminal.ansi.dim_green": "#178a69",

        "terminal.ansi.yellow": "#ffd400",
        "terminal.ansi.bright_yellow": "#ffd400",
        "terminal.ansi.dim_yellow": "#b19c00",

        "terminal.ansi.blue": "#3787d6",
        "terminal.ansi.bright_blue": "#5994CE",
        "terminal.ansi.dim_blue": "#0a2f66",

        "terminal.ansi.magenta": "#EA00D9",
        "terminal.ansi.bright_magenta": "#EA00D9",
        "terminal.ansi.dim_magenta": "#ac077a",

        "terminal.ansi.cyan": "#0ab2fa",
        "terminal.ansi.bright_cyan": "#4bc5fa",
        "terminal.ansi.dim_cyan": "#1183b8",
        "terminal.ansi.white": "#f2f8ff",
        "terminal.ansi.bright_white": "#ffffff",
        "terminal.ansi.dim_white": "#b3c4d1",

        "tooltip.background": "#0b1b45",
        "tooltip.border": "#181657",
        "tooltip.foreground": "#ff2e97",

        "link_text.hover": "#0ef3ff",

        "players": [
          {
            "cursor": "#ff2e97",
            "background": "#073170",
            "selection": "#07317099"
          }
        ],

        "syntax": {
          "attribute": { "color": "#ffd400" },
          "boolean": { "color": "#ff2e97" },
          "comment": { "color": "#0098df", "font_style": "italic" },
          "comment.doc": { "color": "#0098df", "font_style": "italic" },
          "constant": { "color": "#ff2e97" },
          "constructor": { "color": "#ff2e97" },
          "function": { "color": "#ffd400" },
          "keyword": { "color": "#ff2cf1" },
          "label": { "color": "#ffd400" },
          "link_text": { "color": "#0ef3ff" },
          "link_uri": { "color": "#0ef3ff" },

          "number": { "color": "#ffd400", "font_weight": 700 },
          "operator": { "color": "#3e8efd" },

          "preproc": { "color": "#3dd69c" },

          "punctuation": { "color": "#ff2e97" },
          "string": { "color": "#0ef3ff" },
          "string.escape": { "color": "#c832ff" },
          "string.regex": { "color": "#0ef3ff" },
          "string.special": { "color": "#0ef3ff" },
          "string.special.symbol": { "color": "#0ef3ff" },

          "tag": { "color": "#ff2e97" },

          "text.literal": { "color": "#0ef3ff" },
          "title": { "color": "#ff2e97" },
          "type": { "color": "#ff2cf1", "font_style": "italic" },
          "variable": { "color": "#ff2e97" },
          "variable.special": { "color": "#3ec8ff" },
          "parameter": { "color": "#ff2e97" },
          "parameter.special": { "color": "#3ec8ff" }
        }
      }
    }
  ]
}
